--|AutoFarm gold
getgenv().BAutoFarm1=nil
task.wait()
getgenv().BAutoFarm1=true
local c,o,r,n,f=game.Players.LocalPlayer,game.Players.LocalPlayer.OtherData,Vector3.new(),workspace.BoatStages.NormalStages
f=function(v,s)
	repeat task.wait()if c.Character and(c.Character.PrimaryPart)and BAutoFarm1 then c.Character:SetPrimaryPartCFrame(v.CFrame)c.Character.PrimaryPart.Velocity=r end until s.Value~=''
end
while BAutoFarm1 do
	for i=1,9 do
		local v=n['CaveStage'..tostring(i)].DarknessPart
		f(v,o['Stage'..string.match(v.Parent.Name,'%d+')])
	endw
	local v=n.TheEnd.GoldenChest.Trigger
	f(v,o.End)
	repeat task.wait(1)if not BAutoFarm1 then return end workspace.ClaimRiverResultsGold:FireServer()until o.End.Value==''
end
