local a=loadstring(game:HttpGet"https://pastebin.com/raw/za26qWBJ")()local b=a:Window("discord library")local c=b:Server("Cart Ride Gui","")local d=c:Channel("Main")local e=c:Channel("Extra")local f,g,h,i,i2={},{},{},{},{i2}function NewButton(j)if j.ClassName=="ClickDetector"then if j.Parent.Name=="Up"then table.insert(f,j)elseif j.Parent.Name=="Down"then table.insert(g,j)elseif j.Parent.Name=="On"then table.insert(i2,j)end;table.insert(h,j)elseif j.ClassName=="ProximityPrompt"then table.insert(i,j)end end;for k,j in pairs(workspace:GetDescendants())do NewButton(j)end;workspace.DescendantAdded:Connect(function(j)NewButton(j)end)local l=false;d:Toggle("Spam Up On Carts",false,function(m)l=m;if l then while l and task.wait()do for n,j in pairs(f)do fireclickdetector(j)end end end end)local l=false;d:Toggle("Spam Down On Carts",false,function(m)l=m;if l then while l and task.wait()do for n,j in pairs(g)do fireclickdetector(j)end end end end)local l=false;d:Toggle("Spam Off Carts",false,function(m)l=m;if l then while l and task.wait()do for n,j in pairs(i2)do if j.Color==Color3.fromRGB(40,127,71)then fireclickdetector(j)end end end end end)local l=false;d:Toggle("Spam On Carts",false,function(m)l=m;if l then while l and task.wait()do for n,j in pairs(i2)do if j.Color==Color3.fromRGB(196,40,28)then fireclickdetector(j)end end end end end)local l=false;d:Toggle("Break the Game",false,function(m)l=m;if l then while l and task.wait()do for n,j in pairs(h)do fireclickdetector(j)end;for n,j in pairs(i)do fireproximityprompt(j)end end end end)e:Button("Infinite Yield",function()loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()end)
