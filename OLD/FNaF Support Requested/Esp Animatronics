function SetEsp(a)local b=a.Parent;if not b:FindFirstChild("ESP")and a.ClassName=="Humanoid"then local c=Instance.new('Highlight')c.Name='ESP'c.Enabled=true;c.DepthMode=Enum.HighlightDepthMode.AlwaysOnTop;c.FillColor=Color3.fromRGB(255,0,0)c.FillTransparency=0;c.OutlineTransparency=1;c.Parent=b;c.Adornee=b end end;for d,a in pairs(workspace:GetDescendants())do SetEsp(a)end;workspace.DescendantAdded:Connect(function(a)SetEsp(a)end)
