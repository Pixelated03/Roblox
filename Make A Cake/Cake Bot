getgenv().AutoPlaceCakes=true
local r,vec,E=Vector3.new(),{
Rising=Vector3.new(-341,293,-465),
Mixing=Vector3.new(-257,293,-467),
Shaping=Vector3.new(-152,293,-467),
Baking=Vector3.new(-54,293,-470),
Frosting=Vector3.new(39,293,-469),
Topping=Vector3.new(134,293,-470),
Noob=Vector3.new(219,296,-507)}
function PlaceCake(v,p)
	local c=E.Parent.Parent.PrimaryPart
	if c and((v.Position-c.Position).Magnitude<30 and(p-c.Position).Magnitude<30)then
		E:InvokeServer('RequestGrab',v)
		E:InvokeServer('RequestPlace',p,r)
		return true
	end
end
while AutoPlaceCakes and task.wait()do
	E=game.Players.LocalPlayer.Character
	if E and E.PrimaryPart and E:FindFirstChild'Grab Tool'then
		E=E['Grab Tool'].Interaction
		for _,v in pairs(workspace.CakeBatter:GetChildren())do
			if not(v:FindFirstChild'Connection'and
				PlaceCake(v,vec.Rising))
			and not(v:FindFirstChild'KneadStudsSide'and
				PlaceCake(v,vec.Mixing))
			and not((#v:GetChildren()-1<=0 or v:FindFirstChild'Sparkles')and
				PlaceCake(v,vec.Shaping))
			and not(v:FindFirstChildOfClass'Texture'and
				PlaceCake(v,vec.Noob))
			and not(v.ClassName=='MeshPart'and(v.TextureID~='')and
				PlaceCake(v,vec.Topping))
			and not((v:FindFirstChild'Snowflakes'or v:FindFirstChild'Fire')and
				(PlaceCake(v,vec.Baking)or PlaceCake(v,vec.Frosting)))then
				for _,ve in pairs(vec)do
					if PlaceCake(v,ve)then
						break
					end
				end
			end
		end
	end
end
